<?xml version="1.0" encoding="UTF-8"?>

<!-- ==================================================================== -->
<!-- footer.xsl:  Prints HTML page footer.                                -->
<!-- ==================================================================== -->
<xsl:stylesheet version="1.0"
                xmlns:xsl="http://www.w3.org/1999/XSL/Transform"
                xmlns="http://www.w3.org/1999/xhtml">

  <xsl:template name="footer">
    <table width="100%">
      <tr align="right">
        <td>
          <br/><br/><br/>
          <hr/>
          <p class="footer">
            <a href="http://inca.sdsc.edu">
              <img src="/inca/img/inca-powered-by.jpg" border="0"/>
            </a>
          </p>
        </td>
      </tr>
    </table>
  </xsl:template>

</xsl:stylesheet>
