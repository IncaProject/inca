               <value>0</value>
             </arg>
             <arg>
               <name>verbose</name>
               <value>1</value>
             </arg>
             <arg>
               <name>version</name>
               <value>no</value>
             </arg>
          </args>
          <limits>
            <cpuTime>60</cpuTime>
          </limits>
          <context>@@</context>
      </series>
      <resourceSetName>localhost</resourceSetName>
      <schedule>
        <cron>
